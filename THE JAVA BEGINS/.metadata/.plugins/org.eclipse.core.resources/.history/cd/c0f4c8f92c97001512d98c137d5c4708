package com.hrs;

import java.util.Scanner;

public class Prime {
	private static Scanner s;

	public static void main(String[] args) {
		int count;
		s = new Scanner(System.in);
		int i=1;
		System.out.println("Enter the prime till:");
		count=s.nextInt();
		{
			for(int j=1;j<count;j++)
			{
				if(i%j==0&&i%1==0)
				{
					System.out.print(i+",");
				}
				i++;
			}
		}
	}

}
