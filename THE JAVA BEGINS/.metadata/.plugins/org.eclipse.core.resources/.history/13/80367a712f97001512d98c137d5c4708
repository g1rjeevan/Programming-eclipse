package com.hrs;

import java.util.Scanner;

public class Prime {
	private static Scanner s;

	public static void main(String[] args) {
		int count;
		s = new Scanner(System.in);
		int i=4;
		System.out.println("Enter the prime till:");
		count=s.nextInt();
		{
			for(int j=1;j<count;j++)
			{
				if(count>3)
				{   
					System.out.print("0,1,2,3,");
				}
				if(i%2!=0&&i%3!=0)
				{   
					System.out.print(i+",");
				}
				i++;
			}
		}
	}

}
