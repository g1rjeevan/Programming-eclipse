package com.hrs;

public class Selectionsorting {

	public static void main(String[] args)
	
	{
		int arr[]={10,25,0,3,1};
		
		for(int i=0;i<arr.length;i++)
		{
			for(int j=0;j<arr.length;j++)
			{
			 if(arr[i]>arr[j])
			 {
				 int t=arr[i];
				 arr[i]=arr[j];
				 arr[j]=t;
			 }
			}
		}
		for(int i=0;i<arr.length;i++)
		{
			System.out.println(arr[i]);
		}
			
	}
}
