package com.hrs;

public class Array_element_repeat_deleter {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int arr[]={1,2,2,3,3,4,4,4,5,5,5},count=0;
		int j;
		for(int i=0;i<arr.length-1;)
		{
			for(j=i+count;j<arr.length-1;j++)
			{
				if(arr[i]==arr[j])
				{
					count++;
				}
				
				
			}
			if(j==arr.length-1&&i<=arr.length-1)
			{
				i=i+count;
			}
			System.out.println(count);
			count=1;
			System.out.println();
			
			
		}

	}

}
