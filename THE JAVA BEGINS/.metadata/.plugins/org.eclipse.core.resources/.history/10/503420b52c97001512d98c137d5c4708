package com.hrs;

import java.util.Scanner;

public class Prime {
	public static void main(String[] args) {
		int count;
		Scanner s = new Scanner(System.in);
		int i=0;
		System.out.println("Enter the prime till:");
		count=s.nextInt();
		{
			for(int j=1;j<count;j++)
			{
				if(i%2==0&&i%1==0)
				{
					System.out.print(i+",");
				}
			}
		}
	}

}
