package com.hrs;

public class Pattern1 {

	public static void main(String[] args) {
		int row=10; int column=10,mid=column/2;
		for(int i=1;i<=row;i++)
		{
			for(int j=1;j<=column;j++)
			{
				System.out.print(i);
			}
		    System.out.println();
		    if(i<mid){
		    column--;
		    
		}
		    else
		    	column++;
		}
	}

}
