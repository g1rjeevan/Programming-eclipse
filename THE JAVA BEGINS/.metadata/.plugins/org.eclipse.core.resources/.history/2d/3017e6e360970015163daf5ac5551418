package com.hrs;

public class Array_element_repeat_deleter {

	public static void main(String[] args) {

		int arr[]={1,2,2,2,2,3,3};
		int count=1;
		for(int i=0;i<arr.length-1;i++)

		{
			for(int j=0;j<arr.length-1;j++)
			{
				if(i==j)
					j++;
				if(arr[i]==arr[j])
					count++;
			}
			System.out.println(count);
			
			count=1;
		
}
}
}
