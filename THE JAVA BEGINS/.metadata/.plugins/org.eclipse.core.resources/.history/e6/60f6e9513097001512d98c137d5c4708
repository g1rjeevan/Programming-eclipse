package com.hrs;

import java.util.Scanner;

public class Prime {
	private static Scanner s;

	public static void main(String[] args) {
		int count;
		s = new Scanner(System.in);
		int i=4;
		System.out.println("Enter the prime till:");
		count=s.nextInt();
		while(count<10)
		{
			for(int j=2;j<count;j++)
			{
				
				if(count%j!=0)
				{   
					
					System.out.print(i+",");
				}
				
			}
			count++;
		}
	}

}
