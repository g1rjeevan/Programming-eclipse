class Pattern10
{
	public static void main(String[] args) 
	{
		int row=13,column=13;
		int mid=(row+1)/2;
	 for(int i=1;i<=row;i++)
		{
		 for(int j=1;j<=column;j++)
			{
			     if (i==mid||j==mid||i==1||j==1||i==row||j==column||i==j||i==(column-j+1))
			     {
					 System.out.print("*");
			     }
				 else
					 System.out.print(" ");
            }
			System.out.println();

		}
	}
}