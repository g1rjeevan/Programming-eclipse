class Pattern7 
{
	public static void main(String[] args) 
	{
		int row=13,column=13;
	 for(int i=1;i<=row;i++)
		{
		 for(int j=1;j<=column;j++)
			{
			     if (i==j||i==column-j)
			     {
					 System.out.print("*");
			     }
				 else
					 System.out.print(" ");
            }
			System.out.println();

		}
	}
}