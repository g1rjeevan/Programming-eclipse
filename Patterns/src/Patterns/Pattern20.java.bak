class Pattern20
{
	public static void main(String[] args) 
	{    int row=10,col=10,mid=col/2,star=1,space=4;
		for(int i=0;i<=row;i++)
		{
			for (int j=0;j<=col;j++)
			{
			for(int k=0;k<star;k++)
				{
                 System.out.print("*");
				}
				if(mid>=i)
				{
					star++;
					
				}
				else
				{
					star--;
				}
				System.out.print();
            }
		}
		
	}
}
