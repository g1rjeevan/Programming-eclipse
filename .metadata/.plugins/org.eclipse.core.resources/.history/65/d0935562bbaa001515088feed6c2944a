package libraryArray;

import java.util.Arrays;
import java.util.*;

public class StudentArraymain extends StudentArray {

	
	public StudentArraymain(int id, String name) {
		super(id, name);
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("------------------------");
		
		StudentArray[] stArr;
		stArr=new StudentArray[5];
	
		stArr[0]= new StudentArray(1,"Manga");
		stArr[1]= new StudentArray(2,"conga");
		stArr[2]= new StudentArray(3,"sanga");
		stArr[3]= new StudentArray(4,"tunga");
		stArr[4]= new StudentArray(5,"ranga");
		System.out.println("ID\tName");
		System.out.println("------------------------");
		for(int i=0;i<stArr.length;i++){
			//stArr[i].study();
			System.out.println(stArr[i]);
			
			
		}
		
		Arrays.sort(stArr);
		for(int i=0;i<stArr.length;i++){
			//stArr[i].study();
			System.out.println(stArr[i]);
			
			
		}
		
		System.out.println("------------------------");
	}

}
