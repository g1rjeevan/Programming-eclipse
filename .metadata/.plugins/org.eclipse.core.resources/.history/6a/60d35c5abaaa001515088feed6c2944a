package libraryArray;

public class StudentArray {

	int id;
	String name;
	StudentArray(int id, String name){
		this.id=id;
		this.name=name;
		
	}
    void study(){
    	
    	
    	
    	System.out.println(this.id+"\t"+this.name);
    	
    	}
   public String toString(){
    	//overriding the toString() to return the required 
   // return "Student[id="+id+" and name is "+name+"]";
   // 	return "            ";
	   return this.id+"\t"+this.name;
    }
}
