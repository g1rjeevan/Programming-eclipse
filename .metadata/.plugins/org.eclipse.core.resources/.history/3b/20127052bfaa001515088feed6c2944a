package libraryArray;

public class Employee implements Comparable{

	int id;
	String name;
	double salary;
	public Employee(int id,String name,double salary){
		this.id=id;
		this.name=name;
		this.salary=salary;
	}
	public int CompareTo(object arg){
		Employee e1=(Employee)arg;
		return this.id-e1.id;
	}
}
