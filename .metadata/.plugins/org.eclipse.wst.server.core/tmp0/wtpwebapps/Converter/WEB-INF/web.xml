<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Converter</display-name>
	<welcome-file-list>
		<welcome-file>converterPage.html</welcome-file>

	</welcome-file-list>
	-
	<servlet>
		<servlet-name>us</servlet-name>
		<servlet-class>com.g1.UScurrency</servlet-class>
		<init-param>
			<param-name>dollor</param-name>
			<param-value>56.0</param-value>
		</init-param>
	</servlet>
	-
	<servlet-mapping>
		<servlet-name>us</servlet-name>
		<url-pattern>/dollar</url-pattern>
	</servlet-mapping>

</web-app>